<center>
<div class="page-header" style="max-width: 400px">
    <h1>Mitch <br><small>post and share great music</small></h1><br>
    <%= link_to "new song", new_song_path, class: "btn btn-primary btn-lg"%>
</div>


<h4>post and share your favorite music!</h4>
<br>
<div class="list-group" style="max-width: 600px">
  <% @latest_songs.each do |song| %> 
    <%= link_to "#{song.title} - #{song.artist}", song, class: "list-group-item" %>
  <% end %>
</div>
</center>